{
	"name": "hypixel.ts",
	"version": "2.0.0",
	"description": "A simple, easy to use wrapper for Hypixel API.",
	"homepage": "https://hypixel.js.org/",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"author": "FC5570",
	"license": "MIT",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"gen-esm": "yarn gen-esm-wrapper dist/index.js dist/index.mjs",
		"build": "yarn tsc -b src",
		"lint": "yarn eslint src --fix --ext .ts",
		"format": "yarn prettier --write src",
		"test": "yarn vitest",
		"coverage": "yarn test --coverage",
		"prepare": "husky",
		"prepublishOnly": "yarn build && yarn gen-esm",
		"bump": "yarn cliff-jumper",
		"release": "git push && git push --tags && npm publish"
	},
	"dependencies": {
		"@sapphire/async-queue": "^1.5.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.2.1",
		"@commitlint/config-conventional": "^19.1.0",
		"@favware/cliff-jumper": "^3.0.1",
		"@sapphire/eslint-config": "^5.0.4",
		"@sapphire/prettier-config": "^2.0.0",
		"@sapphire/ts-config": "^5.0.1",
		"@types/node": "^20.12.5",
		"@typescript-eslint/eslint-plugin": "^7.5.0",
		"@typescript-eslint/parser": "^7.5.0",
		"@vitest/coverage-c8": "^0.33.0",
		"@vitest/coverage-v8": "^1.4.0",
		"eslint": "^8.57.0",
		"eslint-plugin-prettier": "^5.1.3",
		"gen-esm-wrapper": "^1.1.3",
		"git-cliff": "^2.2.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"typedoc": "^0.25.12",
		"typescript": "^5.4.4",
		"vitest": "^1.4.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FC5570/hypixel.ts"
	},
	"bugs": {
		"url": "https://github.com/FC5570/hypixel.ts/issues"
	},
	"keywords": [
		"hypixel",
		"hypixel-api",
		"hypixel-api-wrapper",
		"hypixel.js",
		"hypixel.ts"
	],
	"files": [
		"dist/",
		"*.md",
		"!README.md"
	],
	"lint-staged": {
		"*.{ts,js}": "yarn lint"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"packageManager": "yarn@4.1.1",
	"prettier": "@sapphire/prettier-config"
}
